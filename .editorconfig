# Top-most editorconfig file

root = true

[*]
end_of_line              = lf
tab_width                = 4
indent_size              = 4
indent_style             = space
trim_trailing_whitespace = true
insert_final_newline     = true

[Jenkinsfile*]
tab_width                = 2
indent_size              = 2

[*.md]
max_line_length          = 70

[*.bat]
end_of_line              = crlf
